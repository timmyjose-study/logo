; an anonymous function is simply a list, in a particular context which takes a lambda

to hangword :secret :guessed
  output map [ifelse memberp ? :guessed [?] ["_]] :secret
end

to get.beatles :names
  output filter [memberp ? [John Paul George Ringo]] :names
end

to biggest :nums
  output reduce [ifelse greaterp ?1 ?2 [?1] [?2]] :nums
end