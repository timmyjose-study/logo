to print.from.l.to.h :l :h
  for [i :l :h] ~
    [print i]
end

to collect.from.l.to.h :l :h
  local "result
  make "result []
  for [i :l :h] ~
    [make "result sentence :result i]
  output :result
end

to for.demo.1 
  for [number 4 7] ~
    [print number]
end

to for.demo.2
  for [number 1 10 2] ~
    [print number]
end

to for.demo.3
  for [number 10 1] ~
    [print number]
end

to for.demo.4 
  for [number 10 1 -3] ~
    [print number]
end
